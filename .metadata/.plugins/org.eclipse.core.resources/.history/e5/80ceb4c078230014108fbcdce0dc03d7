$('project.type').ready(
		function() {
			//取得学校放入下拉框
			$.ajax({
				type : "post",
				url : "getprojecttype.action",
				dataType : "json",
				success : function(data) {
					for ( var i = 0; i < data.length; i++) {
						var sname = data[i];
						var object = document.getElementById('project.type');
						if(!(object.options[0].value == sname))
						{
							document.getElementById('project.type').options.add(new Option(sname, sname));
						}
					}
				},
				error : function(XMLHttpRequest, textStatus, errorThrown) {
					alert(" ddd" + "ajax取学校信息出错-" + errorThrown + "fff");
				}
			});
		});

$('project.enterprise').ready(
		function() {
			//取得学校放入下拉框
			$.ajax({
				type : "post",
				url : "getprojectenterprise.action",
				dataType : "json",
				success : function(data) {
					for ( var i = 0; i < data.length; i++) {
						var sname = data[i];
						var object = document.getElementById('project.enterprise');
						if(!(object.options[0].value == sname))
						{
							document.getElementById('project.enterprise').options
							.add(new Option(sname, sname));
						}
					}
				},
				error : function(XMLHttpRequest, textStatus, errorThrown) {
					alert(" ddd" + "ajax取学校信息出错-" + errorThrown + "fffff");
				}
			});
		});

$('project.state').ready(
		function() {
			//取得学校放入下拉框
			$.ajax({
				type : "post",
				url : "getprojectstate.action",
				dataType : "json",
				success : function(data) {
					for ( var i = 0; i < data.length; i++) {
						var sname = data[i];
						var object = document.getElementById('project.state');
						if(!(object.options[0].value == sname))
						{
							document.getElementById('project.state').options
								.add(new Option(sname, sname));
						}
					}
				},
				error : function(XMLHttpRequest, textStatus, errorThrown) {
					alert(" ddd" + "ajax取学校信息出错-" + errorThrown + "ddddd");
				}
			});
		});

$('project.ispublic').ready(
		function(){
			var object = document.getElementById('project.ispublic');
			if(object.options[0].value == "否"){
				this.options.add(new Option("是","是"));
			}
			else{
				this.options.add(new Option("否","否"));
			}
		});
